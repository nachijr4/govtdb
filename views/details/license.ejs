<% include ../partials/header.ejs %>

<link rel="stylesheet" href="/stylesheets/license.css">

<div class="container">

  <div class="card">
    <div class="card-header">
      License
    </div>
    <div class="card-image">
      <img src="<%= currentUser.image %>" alt=" License Image">
    </div>

    <ul class="list-group list-group-flush">
      <li class="list-group-item">  License Number: <%= license.l_no %></li>
    <li class="list-group-item">Name: <%= currentUser.name %></li>
    <li class="list-group-item">Address: <%= currentUser.address %></li>
    <li class="list-group-item">Dob: <%= currentUser.dob %></li>
    <li class="list-group-item">  Phone: <%= currentUser.p_no %></li>
    <li class="list-group-item">Email: <%= currentUser.email %></li>
    <li class="list-group-item">  Sex: <%= currentUser.sex %></li>
  </ul>

</div>


<% include ../partials/footer.ejs %>

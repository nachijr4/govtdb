<% include ../partials/header.ejs %>

<link rel="stylesheet" href="/stylesheets/viewaadhar.css">


<div class="container">

  <div class="card">
    <div class="card-header">
      Aadhar
    </div>
    <div class="card-image">
      <img src="<%= foundaadhar.image %>" alt=" Aadhar Image">
    </div>

    <ul class="list-group list-group-flush">
      <li class="list-group-item">  Aadhar Number: <%= foundaadhar.aadharNo %></li>
    <li class="list-group-item">Name: <%= currentUser.name %></li>
    <li class="list-group-item">Address: <%= currentUser.address %></li>
    <li class="list-group-item">Dob: <%= currentUser.dob %></li>
    <li class="list-group-item">  Phone: <%= currentUser.p_no %></li>
    <li class="list-group-item">Email: <%= currentUser.email %></li>
    <li class="list-group-item">  Sex: <%= foundaadhar.sex %></li>
  </ul>

</div>


<% include ../partials/footer.ejs %>
